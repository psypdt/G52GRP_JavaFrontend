<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.TilePane?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gui.startScreen.StartScreenController">
   <content>
      <TilePane alignment="CENTER" hgap="25.0" vgap="25.0">
         <children>
             <Button fx:id="open_google" mnemonicParsing="false" text="Open Google" />
             <Button fx:id="open_moodle" mnemonicParsing="false" text="Open Moodle" />
             <Button mnemonicParsing="false" text="Button" />
         </children>
         <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </padding>
      </TilePane>
   </content>
</ScrollPane>
